{
  "url": "${APP_URL}",
  "database": {
    "client": "postgres",
    "connection": {
      "connectionString": "${DATABASE_URL}"
    }
  },
  "server": { "host": "0.0.0.0", "port": 2368 },
  "logging": { "level": "info" },
  "storage": {
    "active": "s3",
    "s3": {
      "accessKeyId": "${S3_ACCESS_KEY}",
      "secretAccessKey": "${S3_SECRET_KEY}",
      "bucket": "${S3_BUCKET}",
      "region": "${S3_REGION}",
      "endpoint": "${S3_ENDPOINT}",
      "forcePathStyle": true
    }
  }
}
